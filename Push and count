<h2>JavaScript Array Methods</h2> 

<h2>push()</h2>



<button onclick="myFunction()">Click</button>

<p id="demo1"></p>
<p id="demo2"></p>

<script>
var things = ["Computer", "Glasses", "Phone", "Guitar"];
document.getElementById("demo1").innerHTML = things;

function myFunction() {
  document.getElementById("demo2").innerHTML = things.push("Pen");
  document.getElementById("demo1").innerHTML = things;
}
</script>

